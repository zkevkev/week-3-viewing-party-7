<% if current_user %>
    <%= button_to "Log Out", logout_path, method: :post %>
<% else %>
    <%= button_to "Create New User", register_path, method: :get %>
    <%= button_to "Login", login_path, method: :get %>
<% end %>

<% if current_user %>
    <section class='existing-users'>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li><%= user.email %></li>
            <%# <li><%= link_to user.email, user_path(user.id) %></li> %>
        <% end %>
    </section>
<% end %>